  "dependencies": {
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.1",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.126",
    "@types/react": "^19.1.12",
    "@types/react-dom": "^19.1.9",
    "axios": "^1.11.0",
    "framer-motion": "^12.23.12",
    "react": "^19.1.1",
    "react-compare-slider": "^3.1.0",
    "react-countup": "^6.5.3",
    "react-dom": "^19.1.1",
    "react-helmet-async": "^1.3.0",
    "react-icons": "^5.0.1",
    "react-intersection-observer": "^9.16.0",
    "react-parallax-tilt": "^1.7.306",
    "react-router-dom": "^7.8.2",
    "react-scripts": "5.0.1",
    "react-toastify": "^11.0.5",
    "react-typed": "^2.0.12",
    "recharts": "^3.1.2",
    "swiper": "^11.2.10",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "build:prod": "node scripts/build.js",
    "build:analyze": "npm run build:prod -- --analyze",
    "test": "craco test",
    "test:coverage": "craco test --coverage --watchAll=false",
    "test:ci": "CI=true npm run test:coverage",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src/ --ext .js,.jsx,.ts,.tsx --fix",
    "type-check": "tsc --noEmit",
    "audit": "npm audit --audit-level moderate",
    "audit:fix": "npm audit fix",
    "serve": "npx serve -s build -l 3000",
    "deploy": "node scripts/deploy.js",
    "deploy:staging": "DEPLOY_ENV=staging npm run deploy",
    "deploy:audit": "npm run deploy -- --audit",
    "precommit": "npm run lint && npm run type-check && npm run test:ci",
    "prebuild": "npm run type-check",
    "postbuild": "npm run audit",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.28.3",
    "@craco/craco": "^7.1.0",
    "autoprefixer": "^10.4.21",
    "cssnano": "^7.1.1",
    "imagemin": "^9.0.1",
    "imagemin-cli": "^8.0.0",
    "preload-webpack-plugin": "^2.3.0",
    "webpack-bundle-analyzer": "^4.10.2"
  }
}
